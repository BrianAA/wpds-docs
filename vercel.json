{
	"rewrites": [
		{
			"source": "/v0/:match*",
			"destination": "https://build.washingtonpost.com/:match*"
		},
		{
			"source": "/assets/:match*",
			"destination": "https://build.washingtonpost.com/assets/:match*"
		},
        {
			"source": "/img/:match*",
			"destination": "https://build.washingtonpost.com/img/:match*"
		}
	]
}
